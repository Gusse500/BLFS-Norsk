        </seg>
        <seg>
          /usr/lib/gnome-maps og
          /usr/share/gnome-maps
        </seg>
      </seglistitem>
    </segmentedlist>

    <variablelist>
      <bridgehead renderas="sect3">Korte Beskrivelser</bridgehead>
      <?dbfo list-presentation="list"?>
