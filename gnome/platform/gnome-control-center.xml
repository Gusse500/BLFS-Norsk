          </para>
          <indexterm zone="gnome-control-center gnome-control-center-prog">
            <primary sortas="b-gnome-control-center">gnome-control-center</primary>
          </indexterm>
        </listitem>
      </varlistentry>

    </variablelist>

  </sect2>

