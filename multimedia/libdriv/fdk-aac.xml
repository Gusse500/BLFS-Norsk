<sect1 id="fdk-aac" xreflabel="fdk-aac-&fdk-aac-version;">
  <?dbhtml filename="fdk-aac.html"?>


  <title>fdk-aac-&fdk-aac-version;</title>

  <indexterm zone="fdk-aac">
