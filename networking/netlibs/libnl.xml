    </para>

  </sect2>

  <sect2 role="content">
    <title>Innhold</title>

    <segmentedlist>
      <segtitle>Installerte Programmer</segtitle>
      <segtitle>Installerte Biblioteker</segtitle>
      <segtitle>Installerte Mapper</segtitle>

      <seglistitem>
        <seg>
          genl-ctrl-list,
          idiag-socket-details,
          nl-class-add,
          nl-class-delete,
          nl-classid-lookup,
          nl-class-list,
          nl-cls-add,
          nl-cls-delete,
          nl-cls-list,
          nl-link-list,
          nl-pktloc-lookup,
          nl-qdisc-add,
          nl-qdisc-delete,
          nl-qdisc-list, og 48 andre hjelpeprogrammer med nl- og nf- prefikser
        </seg>
        <seg>
          libnl-3.so,
          libnl-cli-3.so,
          libnl-genl-3.so,
          libnl-idiag-3.so,
          libnl-nf-3.so,
          libnl-route-3.so,
          libnl-xfrm-3.so, og
          cli moduler under /usr/lib/libnl/cli treet
        </seg>
        <seg>
          /etc/libnl,
          /usr/include/libnl3,
          /usr/lib/libnl, og
          /usr/share/doc/libnl-&libnl-version;
        </seg>
      </seglistitem>
    </segmentedlist>

