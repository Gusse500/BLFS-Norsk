    <bridgehead renderas="sect3">lxqt-openssh-askpass Avhengigheter</bridgehead>

    <bridgehead renderas="sect4">Påkrevd</bridgehead>
    <para role="required">
      <xref linkend="liblxqt"/>
    </para>

