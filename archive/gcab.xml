<sect1 id="gcab" xreflabel="gcab-&gcab-version;" revision="systemd">
  <?dbhtml filename="gcab.html"?>


  <title>gcab-&gcab-version;</title>

  <indexterm zone="gcab">
    <primary sortas="a-gcab">gcab</primary>
  </indexterm>

  <sect2 role="package">
    <title>Introduksjon til Gcab</title>

    <para>
      <application>Gcab</application> pakken inneholder et
      program og et bibliotek som brukes til å lage Microsoft kabinett
      (.cab) arkiver.
    </para>

    &lfs81_checked;

    <bridgehead renderas="sect3">Pakkeinformasjon</bridgehead>
    <itemizedlist spacing="compact">
      <listitem>
        <para>
          Nedlasting (HTTP): <ulink url="&gcab-download-http;"/>
        </para>
      </listitem>
      <listitem>
        <para>
          Nedlasting (FTP): <ulink url="&gcab-download-ftp;"/>
        </para>
      </listitem>
      <listitem>
        <para>
          Nedlasting MD5 sum: &gcab-md5sum;
        </para>
      </listitem>
      <listitem>
        <para>
          Nedlastingsstørrelse: &gcab-size;
        </para>
      </listitem>
      <listitem>
        <para>
          Estimert diskplass som kreves: &gcab-buildsize;
        </para>
      </listitem>
      <listitem>
        <para>
          Estimert byggetid: &gcab-time;
        </para>
      </listitem>
    </itemizedlist>

    <bridgehead renderas="sect4">Påkrevd</bridgehead>
    <para role="required">
      <xref linkend="glib2"/>
    </para>

    <bridgehead renderas="sect4">Anbefalt</bridgehead>
    <para role="recommended">
      <xref linkend="gobject-introspection"/> og
      <xref linkend="vala"/>
    </para>

    <bridgehead renderas="sect4">Valgfri</bridgehead>
    <para role="optional">
      <xref linkend="gtk-doc"/>
    </para>

    <para condition="html" role="usernotes">Brukernotater:
      <ulink url="&blfs-wiki;/gcab"/>
    </para>
  </sect2>

  <sect2 role="installation">
    <title>Installasjon av Gcab</title>

    <para>
      Installer <application>Gcab</application> ved å kjøre
      følgende kommandoer:
    </para>

<screen><userinput>./configure --prefix=/usr --disable-static &amp;&amp;
make</userinput></screen>

    <para>
      For å teste resultatene, kjør: <command>make check</command>.
    </para>

    <para>
      Nå, som <systemitem class="username">root</systemitem> bruker:
    </para>

<screen role="root"><userinput>make install</userinput></screen>

  </sect2>

