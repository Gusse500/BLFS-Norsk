    <itemizedlist spacing="compact">
      <listitem>
        <para>Nedlasting (HTTP): <ulink url="&kdelibs-download-http;"/></para>
      </listitem>
      <listitem>
        <para>Nedlasting (FTP): <ulink url="&kdelibs-download-ftp;"/></para>
      </listitem>
      <listitem>
        <para>Nedlasting MD5 sum: &kdelibs-md5sum;</para>
      </listitem>
