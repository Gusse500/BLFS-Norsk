    <itemizedlist spacing='compact'>
      <listitem>
        <para>Nedlasting (HTTP): <ulink url="&kdeedu-download-http;"/></para>
      </listitem>
      <listitem>
        <para>Nedlasting (FTP): <ulink url="&kdeedu-download-ftp;"/></para>
      </listitem>
      <listitem>
        <para>Nedlasting MD5 sum: &kdeedu-md5sum;</para></listitem>
      <listitem>
